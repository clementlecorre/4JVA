<?xml version="1.0"?><!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping>
    <class name="courses" table="courses">
        <id name="idCourses" type="int" column="idCourses">
            <generator class="native"/>
        </id>
        <property name="label" type="string" not-null="true" />
        <property name="rank" type="float" not-null="true" />
        <property name="details" type="Text"></property>
    </class>
</hibernate-mapping>
